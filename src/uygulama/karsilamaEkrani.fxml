<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
          fx:controller="uygulama.KarsilamaKontrolcusu">

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>

    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Button mnemonicParsing="false" onAction="#ElemanGirisi" text="Eleman Girişi" GridPane.halignment="CENTER"
                GridPane.valignment="CENTER"/>
        <Button mnemonicParsing="false" onAction="#SirketGirisi" text="Şirket Girişi" GridPane.columnIndex="1"
                GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
    </children>
</GridPane>
